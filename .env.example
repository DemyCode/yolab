# YoLab Environment Variables
# Copy this file to .env and fill in your values

# PostgreSQL Configuration
POSTGRES_DB=frp_services
POSTGRES_USER=frp_user
POSTGRES_PASSWORD=your_secure_password_here

# Domain Configuration
DOMAIN=yourdomain.com

# FRP Server Configuration
FRPS_SERVER_IPV6=2001:db8::1
FRPS_SERVER_PORT=7000

# IPv6 Subnet for Client Allocation
# Format: 2001:db8::1:0:0:0 (without /64)
IPV6_SUBNET_BASE=2001:db8::1:0:0:0

# Backend API Configuration
REGISTRATION_API_HOST=0.0.0.0
REGISTRATION_API_PORT=5000
